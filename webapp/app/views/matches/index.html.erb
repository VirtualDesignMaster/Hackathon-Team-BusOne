<p id="notice"><%= notice %></p>

<h1>User Match</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
      </tr>
    <% end %>
    <tr>
      <td><%= @result %></td>
    </tr>
  </tbody>
</table>

<br>

<%= link_to 'New Match', new_match_path %>
